{
  "name": "tree-sitter-strudel",
  "version": "0.2.1",
  "description": "Strudel grammar for tree-sitter",
  "repository": "https://github.com/ntsk/tree-sitter-strudel",
  "license": "MIT",
  "author": {
    "name": "ntsk",
    "email": "ntsk@ntsk.jp",
    "url": "https://ntsk.jp/"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "strudel"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter": "^0.25.0",
    "tree-sitter-cli": "^0.26.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4 || ^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  }
}
